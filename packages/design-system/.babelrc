{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "env": {
      "test": {
        "presets": [
          ["@babel/preset-env", { "modules": "cjs" }]
        ]
      },
      "build": {
         "presets": [
            ["@babel/preset-env", { "modules": false }]
          ]
       }
    },
    "plugins": [[
      "babel-plugin-module-resolver",
      {
        "root": [
          "./src"
        ]
      }
    ]]
  }